# Reload sxhkd
super + shift + Escape
	pkill -USR1 -x sxhkd

#VOLUME KEYS
XF86AudioRaiseVolume
	changevolume up

XF86AudioLowerVolume
	changevolume down

XF86AudioMute
	changevolume mute


#BRIGHTNESS KEYS
XF86MonBrightnessUp
	changebrightness up

XF86MonBrightnessDown
	changebrightness down

#DESKTOP KEYS
super + Return
  st
super + b 
  brave --incognito
super + shift + b 
  brave 
super + shift +a 
  st -e cmus
super + e
  st -e lf
super + control + q
  slock
  
